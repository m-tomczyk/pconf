#!/dev/null
# Listing stuff
alias ll='ls -alFh'
alias la='ls -A'
alias l='ls -CF'

# Easier navigation
alias cd..='cd ..'
alias ..='cd ..'
alias ...='cd ../..'
alias .1='cd ..'
alias .2='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'

# Mkdir not annoying
alias mkdir='mkdir -p'

# Make disk utils speak human
alias df='df -H'
alias du='du -ch'

# Docker
alias dps='docker ps'

# yalc
alias yra='yalc remove --all'